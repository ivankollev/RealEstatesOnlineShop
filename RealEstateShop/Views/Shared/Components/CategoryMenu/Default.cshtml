
<li class="dropdown">
    <a asp-controller="RealEstate"
       asp-action="Index"
       class="dropdown-toggle" data-toggle="dropdown">Real Estates<b class="caret"></b></a>
    <ul class="dropdown-menu">
        @foreach (var category in Model)
        {
            <li>
                <a asp-controller="RealEstate" asp-action="List"
                   asp-route-category="@category.Description">@category.Description</a>
            </li>
        }
        <li class="divider"></li>
        <li>
            <a asp-controller="RealEstate" asp-action="List" asp-route-category="">View all Real Estates</a>
        </li>
    </ul>
</li>